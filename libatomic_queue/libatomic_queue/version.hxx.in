#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBATOMIC_QUEUE_VERSION       $libatomic_queue.version.project_number$ULL
#define LIBATOMIC_QUEUE_VERSION_STR   "$libatomic_queue.version.project$"
#define LIBATOMIC_QUEUE_VERSION_ID    "$libatomic_queue.version.project_id$"

#define LIBATOMIC_QUEUE_VERSION_MAJOR $libatomic_queue.version.major$
#define LIBATOMIC_QUEUE_VERSION_MINOR $libatomic_queue.version.minor$
#define LIBATOMIC_QUEUE_VERSION_PATCH $libatomic_queue.version.patch$

#define LIBATOMIC_QUEUE_PRE_RELEASE   $libatomic_queue.version.pre_release$

#define LIBATOMIC_QUEUE_SNAPSHOT_SN   $libatomic_queue.version.snapshot_sn$ULL
#define LIBATOMIC_QUEUE_SNAPSHOT_ID   "$libatomic_queue.version.snapshot_id$"
